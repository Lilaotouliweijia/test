<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="entity">
	<class name="Xcsj" table="xcsj">
		<id name="xcSheJ" type="integer">
			<column name="xcSheJ" />
			<generator class="identity"></generator>
		</id>
		<property name="xcShenJtitle" type="string" length="20">
			<column name="xcShenJtitle"></column>
		</property>
		<property name="xcFind" type="string" length="255">
			<column name="xcFind"></column>
		</property>
		<property name="question" type="string" length="255">
			<column name="question"></column>
		</property>
		<property name="url" type="string" length="100">
			<column name="url"></column>
		</property>
	</class>
</hibernate-mapping>
