<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- struts配置文件的根 -->
<struts>
	<package name="ma" namespace="/" extends="main">
		<action name="zskAll" class="zskAction" method="zskAll">
			<result name="success">/main.jsp</result>
		</action>
		<action name="delzsk" class="zskAction" method="delzsk">
			<result name="success" type="redirectAction">zskAll</result>
		</action>
	</package>
</struts>

		
